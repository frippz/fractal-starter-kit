{
  "env": {
    "amd": true,
    "browser": true,
    "node": true
  },
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "requirejs"
  ],
  "rules": {
    "strict": 1,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": [2, {
      "properties": "always"
    }],
    "comma-spacing": [2, {
      "before": false, "after": true
    }],
    "comma-style": [2, "last"],
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 2
    }],
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true
    }],
    "new-cap": 2,
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": [2, {
      "exceptions": {
        "VariableDeclarator": true,
        "Property": true
      }
    }],
    "no-multiple-empty-lines": [2, { "max": 1 }],
    "no-trailing-spaces": 2,
    "no-redeclare": 2,
    "no-undef": 2,
    "no-underscore-dangle": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "object-curly-spacing": [2, "always", {
      "objectsInObjects": false,
      "arraysInObjects": false
    }],
    "quotes": [2, "single", "avoid-escape"],
    "radix": 2,
    "semi": 2,
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "keyword-spacing": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
    "space-infix-ops": 2,
    "space-in-parens": [2, "never"],
    "spaced-comment": [2, "always"],
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }]
  }
}
